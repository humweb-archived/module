{
    "name": "humweb/module",
    "description": "Module system for laravel framework",
    "keywords": ["module", "laravel"],
    "license": "MIT",
    "homepage": "https://github.com/humweb/module",
    "authors": [
        {
            "name": "humweb shofner",
            "email": "humweb@humboldtweb.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "4.1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.8.0",
        "phpunit/php-code-coverage": "1.2.*@dev",
        "phpunit/phpunit": "3.7.18",
        "phpunit/dbunit": "1.2.3",
        "phpmd/phpmd": "1.5.0",
        "sebastian/phpcpd": "1.4.3",
        "squizlabs/php_codesniffer": "1.4.6",
        "codeception/codeception": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Humweb\\Module\\": "src/",
            "Teststubs": "tests/"
        }
    },
    "minimum-stability": "dev"
}
